local Library = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local Window = Library:CreateWindow({
    Title = "The Library " .. Library.Version,
    SubTitle = "by Godlike",
    TabWidth = 160,
    Size = UDim2.fromOffset(400, 162),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when there's no MinimizeKeybind
})

-- Fluent provides Lucide Icons, they are optional
local Tabs = {
    ["THE LIBRARY"] = Window:AddTab({ Title = "THE LIBRARY", Icon = "0" }),
}

-- Correctly access the tab from the Tabs table
local libraryTab = Tabs["THE LIBRARY"]

libraryTab:AddButton({
    Title = "MoonHub Elerium",
    Description = "Opens MoonHub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/MoonLightYT/MoonHub-Elerium/refs/heads/main/GOD-PROTECTION100%25SECURE100%25SECURE.txt"))()   
    end
})

libraryTab:AddButton({
    Title = "TGX Script",
    Description = "Opens TGX Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/MoonLightYT/TGX-PUBLIC/refs/heads/main/-----100%25GOD-PROTECT100%25-----.txt"))()  
    end
})

libraryTab:AddButton({
    Title = "NW HUB",
    Description = "Opens NW Script",
    Callback = function()
         loadstring(game:HttpGet('https://raw.githubusercontent.com/Ceedif/Nw_Hub/refs/heads/main/Nw_Script'))() 
    end
})

libraryTab:AddButton({
    Title = "Beamed v2",
    Description = "Opens Beamed V2",
    Callback = function()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/ben1x1x1x1x/V2/refs/heads/main/BeamedV2FreeLoader"))()
   end
})

libraryTab:AddButton({
    Title = "Lunari",
    Description = "It opens Lunari",
    Callback = function()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/TryharderKid/Test/refs/heads/main/Protected_3990809938452538.txt"))()
    end
})

libraryTab:AddButton({
    Title = "AX HUB",
    Description = "This opens AX HUB",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/W4ttziez/Muscle-Legends/main/AX%20HUB.lua"))()
    end
})

libraryTab:AddButton({
    Title = "Enchanted Hub",
    Description = "This opens Enchanted Hub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/iblameaabis/Enchanted/refs/heads/main/Enchanted%20Hub%20On%20Top"))()
    end
})

libraryTab:AddButton({
       Title = "OP BOOSTER SCRIPT---BY MOHA",
       Description = "This eats every booster in ur inventory",
       Callback = function()
           loadstring(game:HttpGet("https://raw.githubusercontent.com/MoonLightYT/All-booster-eat-/refs/heads/main/MOHA-PROTECTED100%25SECURE100%25SECURE.txt.txt"))()
       end
})

local Tabs = {
    ["KeySystems"] = Window:AddTab({ Title = "KeySystems", Icon = "0" }),
}

-- Adding sections to the KeySystems tab
local Section1 = Tabs["KeySystems"]:AddSection("Beamed V2 script = benishot")
local Section2 = Tabs["KeySystems"]:AddSection("Enchanted Hub Key = aabisisgay")
local Section3 = Tabs["KeySystems"]:AddSection("May potentially add a key system to MoonHub but tbh its up to u guys")
